<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<display-name>MobileOperator</display-name>
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
		<welcome-file>login.jsp</welcome-file>
		<welcome-file>registration.jsp</welcome-file>
	</welcome-file-list>

	<servlet>
		<description>Controls and dispatches requests from JSP's</description>
		<display-name>FrontServlet</display-name>
		<servlet-name>FrontServlet</servlet-name>
		<servlet-class>ru.tsystems.ecare.controller.FrontServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>FrontServlet</servlet-name>
		<url-pattern>/dispatcher/</url-pattern>
	</servlet-mapping>
	
</web-app>